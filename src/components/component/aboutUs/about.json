      "id": 10,
      "title": "Our Mandate",
      "description": "The act that governs our operations",
      "items": [
        {
          "id": 37,
          "title": "Issue Ugandan Currency",
          "description": "The only institution allowed to design, print, issue and manage banknotes and coins in Uganda.",
          "image": null
        },
        {
          "id": 38,
          "title": "Formulate and Implement Monetary Policies",
          "description": "Regulates the supply & demand for money in the economy to maintain the value of money and to ensure Price Stability.",
          "image": null
        },
        {
          "id": 39,
          "title": "Overseeing Financial Institutions",
          "description": "Oversees financial institutions to ensure regulations compliance, safeguard deposits, and promote financial system stability.",
          "image": null
        },
        {
          "id": 40,
          "title": "Banker to Government",
          "description": "Provides banking services to Government Ministries, Departments and Agencies.",
          "image": null
        }
      ]
